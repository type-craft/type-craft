  "description": "TypeCraft date type",
  "author": "guillem.cordoba@gmail.com",
  "main": "dist/index.js",
  "module": "dist/index.js",
  "types": "dist/index.d.ts",
  "exports": {
    ".": "./dist/index.js",
    "./create-date": "./dist/definitions/create-date.js",
    "./date-detail": "./dist/definitions/date-detail.js"
