    "lodash-es": "^4.17.21",
    "lit": "^2.1.3"
  },
  "devDependencies": {
    "@custom-elements-manifest/analyzer": "^0.4.17",
    "@rollup/plugin-commonjs": "^21.0.2",
    "@web/dev-server": "^0.1.28",
    "@web/dev-server-rollup": "^0.3.15",
    "rimraf": "^3.0.2",
    "typescript": "^4.4.3"
  },
